<div class="row">
  <div class="span14">
<ol class="media-grid">
  <% artists.first(24).each do |artist| %>
    <li>
      <a class="artist-image" <% if artist.mbid %>href="/artists/<%= artist.mbid%>"<% end %>>
        <img class="thumbnail" onhover="" <% if artist.mbid %>id="<%= artist.mbid %>"<% end %> src="" alt="<%= artist.name + ": #{pluralize(artist.num_times)}" %>" title="<%= artist.name + ": #{pluralize(artist.num_times)}" %>">
      </a>
    </li>
  <% end %>
</ol>
    </div>
</div>
<div class="row">
  <h5><div class="artist-name">&nbsp;</div></h5>
</div>

